@model IEnumerable<Hotels_Resrevation.Models.Facilties>

@if (Model.Count() > 0)
{
    foreach (var fac in Model)
    {
        <div class="ui card" style="text-align: center;border: 1px solid #ddd;border-radius: 10px;padding: 10px;max-width: 200px;">
            <div>
                <h4>@fac.Name</h4>
            </div>
        </div>
    }
}
else
{
    <h2 style="text-align: center">@Hotels_Resrevation.Resources.Profile.ProfileIndex.No_Facilties</h2>
}
